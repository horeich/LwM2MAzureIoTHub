<?xml version="1.0" encoding="UTF-8"?>

<LWM2M  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
	<Object ObjectType="MODefinition">
		<Name>Level Sensor Device Telemetry</Name>
		<Description1>Level sensor object.</Description1>
		<ObjectID>64000</ObjectID>
		<ObjectURN>urn:oma:lwm2m:ext:64000:1.1</ObjectURN>
		<LWM2MVersion>1.0</LWM2MVersion>
		<ObjectVersion>1.1</ObjectVersion>
		<MultipleInstances>Multiple</MultipleInstances>
		<Mandatory>Optional</Mandatory>
		<Resources>
			<Item ID="1">
				<Name>Timestamp</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Time</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>s</Units>
				<Description>Timestamp of the measurement.</Description>
			</Item>
			<Item ID="2">
				<Name>DeviceTemperature</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>°C</Units>
				<Description>Temperature of the on-board temperature sensor.</Description>
			</Item>
            <Item ID="3">
				<Name>WaterTemperature</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>°C</Units>
				<Description>Temperature of the water.</Description>
			</Item>
			<Item ID="4">
				<Name>WaterPressure</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>mWs</Units>
				<Description>Pressure of the water.</Description>
			</Item>
		</Resources>
		<Description2></Description2>
	</Object>
</LWM2M>
